.col-4.p-2
  .card.feed
    .card-body
      %h5.card-title=feed.name
      - if feed.description
        %p.fs-10.card-text=feed.description
      .d-flex.justify-content-end(id="feed-#{feed.id}")
        - if feed.subscribed?(current_user)
          %i.bi.bi-check-lg
        - else
          = form_with url: admin_subscribe_path(feed) do |form|
            %button(type="submit" class="btn btn-sm btn-outline-primary") Subscribe