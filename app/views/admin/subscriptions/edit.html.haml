- content_for :meta_title do
  Edit RSS subscription

#new-sub
  .p-4.w-100
    %h4
      = link_to "Subscriptions", admin_subscriptions_path
      &gt; Edit

    = form_with(model: @subscription, url: admin_subscription_path(@subscription), method: :patch, local: true) do |form|
      = render form, subscription: @subscription, feed: @subscription.feed, service: @service
      